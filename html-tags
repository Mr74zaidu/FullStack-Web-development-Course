   <!-- Heading Tags -->
<h1>This is h1</h1>
<h2>This is h2</h2>
<h3>This is h3</h3>
<h4>This is h4</h4>
<h5>This is h5</h5>
<h6>This is h6</h6>

   <!-- Paragraph Tag -->
    <p>This is Paragraph tag.</p>

    <!-- Ancher tag -->
     <a href="https://www.w3school.com">Click here This is anchor tag!..</a>

     <!-- THis is link breake tag -->
     <br>    <br>
     
     <!-- If you want to open your ancher link in onther tab Then you need you use this. -->
     <a href="https://www.google.com" target="_blank">Click here and open link in another tab</a><br>

    <!-- image tag if not have immage than print no yet if you want to define width the you ues the width element.-->
    <img width="250" src="download.jpeg" alt="not yet">


<!-- Table Tags -->
 <table>
    <thead>
        <tr>
            <th>Name</th>
            <th>class</th>
            <th>language</th>
        </tr>
    </thead>

    <tbody>
        <tr>
        <td>Zaidu</td>
        <td>BCA</td>
        <td>JavaScript</td>
        </tr>

        <tr>
            <td>Zaidu</td>
            <td>BCA</td>
            <td>JavaScript</td>
            </tr>
    </tbody>
 </table>

 

 <!-- order list -->
  <!-- type="i", I, 1, etc -->
  <ol type="I">
    <h1>Order list</h1>
    <li>India</li>
    <li>Srilanka</li>
    <li>USA</li>
  </ol>

  <!-- Unorder list -->
   <!-- type="square", etc -->
<ul type="square">
    <h1>Unorder list</h1>
    <li>Maharashtra</li>
    <li>Rajisthan</li>
    <li>Goa</li>
</ul>

<!-- Form Tags -->
 <form action="">  <!--In action we can use get and post method-->


    <!-- Input and label Tags -->
    <label for="name"> Name </label>
    <input type="text" id="name" placeholder="Enter your name" autofocus><br><br>

    <!-- input tags in radio button -->
    <input type="radio" id="male" name="gender" value="male">
    <label for="male"> male </label>
    <input type="radio" id="female" name="gender" value="female">
    <label for="female">female</label><br><br>

    <input type="checkbox" id="checkbox" name="checkbox" required>
    <label for="checkbox">this is a check box </label> <br><br>

    <!-- Check box -->
     <label for="cityname">City names</label><br>
    <select name="cityname" id="cityname">
        <option value="Delhi">Delhi</option>
        <option value="mumbais">mumbais</option>
        <option value="kolkata">kolkata</option>
        <option value="jalgaon">jalgaon</option>
    </select><br><br>

    <textarea name="comment" id="textarea" placeholder="Enter your comments here" rows="4"></textarea>
 </form>


<!-- audio tag -->
<audio src="audio.m4a" controls></audio>


<!-- video tag -->
 <video src="video.mp4" controls="controls" type="video/mp4"></video><br>



